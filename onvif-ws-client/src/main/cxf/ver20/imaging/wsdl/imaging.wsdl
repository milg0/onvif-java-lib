					<xs:annotation>
						<xs:documentation>Indicates whether or not Image Stabilization feature is supported.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:anyAttribute processContents="lax"/>
			</xs:complexType>
			<xs:element name="Capabilities" type="timg:Capabilities"/>
			<!--===============================-->
